<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ece8e8"
    android:orientation="vertical">

    <TextView

        android:gravity="center"
        android:text="恭喜你设置完成"
        android:layout_width="match_parent"
        android:layout_height="50sp"

        android:textColor="#ffff66"
        android:textSize="30sp"
        android:background="#99cc66"
        />
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/tv_protect_on_off"
        android:drawableLeft="@android:drawable/checkbox_off_background"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10sp"
        android:layout_marginTop="3sp"
        android:text="你没有开启防盗保护"
        android:textColor="#e705d8"
        android:textSize="20sp" />
</LinearLayout>
<com.lc.mobilesafe.com.lc.uis.pre_next_star_view
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/star4">

</com.lc.mobilesafe.com.lc.uis.pre_next_star_view>

</LinearLayout>